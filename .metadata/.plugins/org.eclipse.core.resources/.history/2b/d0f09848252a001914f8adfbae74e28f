			System.out.print("entrer votre prenom:");
			x=s.nextLine();
			tab[i].setPrenom(x);
			System.out.print("entrer votre date de naissance :");
			x=s.nextLine();
			tab[i].setDateOfBirth(x);
			
		}
		
